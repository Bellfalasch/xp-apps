{
  "name": "app-contentstudio",
  "version": "1.0.0",
  "description": "Enonix XP - Content Studio App",
  "scripts": {
    "build": "webpack --hide-modules",
    "lint": "run-p lint:* -c --aggregate-output",
    "fix": "run-p fix:* -c --aggregate-output",
    "lint:ts": "tslint -c ../../tslint.json -e **/*.d.ts src/**/*.ts",
